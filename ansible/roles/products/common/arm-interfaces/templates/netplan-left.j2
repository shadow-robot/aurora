#jinja2: trim_blocks:False
network:
    ethernets:
        {{ ethercat_left_arm }}:
            addresses: [192.168.2.100/24]
            optional: true
            routes:
                - to: 192.168.2.1/32
                  via: 192.168.2.1
        enx-usb-ethernet:
            match:
                name: enx*
            dhcp4: true
            optional: true
    version: 2
