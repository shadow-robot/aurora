#jinja2: trim_blocks:False
[Desktop Entry]
Version=1.0
Name={{ desktop_shortcut_name }}
Comment={{ comment }}
Exec=terminator -T '{{ terminator_prefix_for_closing }}{{ desktop_shortcut_name }}' -e {{ folder }}/{{ shell_script_file_name }}
Icon={{ folder }}/{{ icon_file_name }}
Terminal={{ start_terminal }}
Type=Application
Categories=Utility;Application;
