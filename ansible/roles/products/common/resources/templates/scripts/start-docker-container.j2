#!/bin/bash
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
NC='\033[0m' # No Color
echo -e \"${YELLOW}Please wait for docker container to start in a new terminal as this might take a while... ${NC}\"
{{ docker_executable }} start {{ container_name  }} &> /dev/null
sleep infinity
